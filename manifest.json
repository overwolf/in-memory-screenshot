{
  "manifest_version":"1",
  "type":"WebApp",
  "meta":{
    "name":"In-Memory Screenshot Sample App",
    "version":"1.0.0.0",
    "minimum-overwolf-version": "0.85.0.0",
    "author":"Overwolf",
    "icon":"icon.png",
    "icon_gray":"icon_gray.png",
    "description":"Sample app for using the In-Memory Screenshot API"
  },
  "data": {
    "start_window":"index",
    "windows":{
      "index":{
        "file":"index.html",
        "transparent":false,
        "resizable":true,
        "show_in_taskbar": true,
        "size": {
          "width": 400,
          "height": 400
        },
        "start_position": {
          "Top": 100,
          "Left": 300
        }
      }
    }
  }
}